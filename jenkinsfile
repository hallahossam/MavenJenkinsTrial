pipeline {
tools{
maven 'Maven 3.8.5'
}

agent any
stages{

stage('Compile Stage'){
steps{

echo "Halla is here 1"
cd "C:/Program Files/MyGitProjects/GitHub/MavenJenkinsTrial/pom.xml"
sh 'mvn clean'
echo "Halla is here 2"
}

}
stage('Testing Stage'){
steps{
echo "Halla is here 3"
sh 'mvn test -Dtest="FizzBuzz"'

}
}
}


}