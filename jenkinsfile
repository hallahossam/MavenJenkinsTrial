pipeline {
tools{
maven 'Maven 3.8.5'
}

agent any
stages{

stage('Compile Stage'){
steps{

echo "Halla is here 1"
dir ("C:/Program Files/MyGitProjects/GitHub/MavenJenkinsTrial"){
bat 'mvn clean compile'
}
echo "Halla is here 2"
}

}
stage('Testing Stage'){
steps{
echo "Halla is here 3"
dir ("C:/Program Files/MyGitProjects/GitHub/MavenJenkinsTrial"){
bat 'mvn test -Dtest="FizzBuzz"'
}

}
}
}


}